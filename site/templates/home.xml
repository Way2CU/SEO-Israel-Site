<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document SYSTEM "../units/schema/language.dtd">
<document>
	<cms:var name="_DOCTYPE"/>
	<html lang="$language" cms:eval="lang">
		<head>
			<title><cms:text constant="site_title"/></title>
			<cms:collection include="page_control"/>
			<cms:module name="head_tag" action="print_tag"/>
		</head>
		<body class="$language_rtl ? 'rtl' : 'ltr'" cms:eval="class">
			<!--Top Section of the site  -->
			<header>
					<div class="inner_wrap">
						 <div class="contact">
								<span><cms:text constant="contact"/></span>
								<cms:desktop>
								<a
									href="'tel:'.urlencode(Language::getText('phone'))"
									cms:eval="href"
									class="phone"
									>
									<cms:text constant="phone"/>
								</a>
								</cms:desktop>
								<cms:mobile>
								<a
									href="'tel:'.urlencode(Language::getText('phone'))"
									cms:eval="href"
									class="phone"
									>

								</a>
								</cms:mobile>
						 </div>
					</div>
					<div class="animation_page">
							<div class="animate">
							 	<div class="inner_wrap">
							 		<h1><cms:text constant="header"/></h1>
 								 		<cms:module
													name="articles"
													action="show"
													text_id="form_article"
													template="parts/article_header.xml"
													/>
 										<cms:module
 									 				name="contact_form"
 									 				action="show"
 									 				text_id="animation_form"
 							 						/>
					 			</div>

							 </div>
							 <div class="animate">
									<div class="inner_wrap">
							 			<div class="progress">
 									 			<div class="meter"><span></span></div>
 									 			<span><cms:text constant="anima1"/></span>
								 		</div>
								 		<div class="progress">
								 			<div class="meter"><span></span></div>
								 			<span><cms:text constant="anima2"/></span>
								 		</div>
								 		<div class="progress">
								 			<div class="meter"><span></span></div>
								 			<span><cms:text constant="anima3"/></span>
								 		</div>
								 		<div class="progress">
								 			<div class="meter"><span></span></div>
								 			<span><cms:text constant="anima4"/></span>
								 		</div>
									</div>
						 	</div>
						 	<div class="animate">
						 			<div class="inner_wrap">
						 				<div class="form_text">
						 					<h2><cms:text constant="userUrl"/></h2>
							 				<span></span>
							 				<span><cms:text constant="form_title"/></span>
							 			</div>
						 				<cms:module
									 				name="contact_form"
									 				action="show"
									 				text_id="contact_form"
 							 						/>
						 			</div>
						 	</div>

						 	<div class="animate">
								<div class="inner_wrap">
						 				<cms:module
											name="articles"
											action="show"
											text_id="success"
											template="parts/article_header.xml"
											/>
								</div>
							</div>
					 </div>
					<div class="header_bottom">
						<h2><cms:text constant="seo_title"/></h2>
					</div>
			</header>
			<!-- FEATURES SECTION -->
			<div class="feature_wrap">
							<div class="feature">
								<cms:module
									name="articles"
									action="show"
									text_id="feature1"
									template="parts/article_header.xml"
									/>

							</div>
							<div class="feature">
								<cms:module
									name="articles"
									action="show"
									text_id="feature2"
									template="parts/article_header.xml"
									/>

							</div>
							<div class="feature">
								<cms:module
									name="articles"
									action="show"
									text_id="feature3"
									template="parts/article_header.xml"
									/>

							</div>
							<div class="feature">
								<cms:module
									name="articles"
									action="show"
									text_id="feature4"
									template="parts/article_header.xml"
									/>

							</div>
			</div>
			<!-- Social Section -->
			<div class="social_wrap">
							<h2><cms:text constant="social_title"/></h2>
							<span></span>
							<span></span>
							<span></span>
							<span></span>
							<span></span>
							<span></span>
							<span></span>
			</div>
			<!-- Testimonial Section -->
			<div class="testimonial_wrap">
					<div class="inner_wrap">
							<h2><cms:text constant="testimonial_title"/></h2>
							<div class="testimonial">
								<cms:module
									name="articles"
									action="show"
									text_id="testimonial_one"
									template="parts/article_noHeading.xml"
									/>

							</div>
							<div class="testimonial">
								<cms:module
									name="articles"
									action="show"
									text_id="testimonial_two"
									template="parts/article_noHeading.xml"
									/>

							</div>
							<div class="testimonial">
								<cms:module
									name="articles"
									action="show"
									text_id="testimonial_three"
									template="parts/article_noHeading.xml"
									/>

							</div>
					</div>
			</div>
			<!-- Clients Section -->
			<div class="client_wrap">
							<span></span>
							<span></span>
							<span></span>
							<span></span>
							<span></span>
							<span></span>
							<span></span>
							<span></span>
							<span></span>
			</div>
			<!-- Footer -->
			<footer>
					<div class="inner_wrap">
							<div class="contact_wrap">
								<div class="contact">
										<cms:module
												name="articles"
												action="show"
												text_id="contact1"
												template="parts/article_header.xml"
												/>
								</div>
								<div class="contact">
										<cms:module
												name="articles"
												action="show"
												text_id="contact2"
												template="parts/article_header.xml"
												/>
								</div>
								<div class="contact">
										<cms:module
												name="articles"
												action="show"
												text_id="contact3"
												template="parts/article_header.xml"
												/>
								</div>
							</div>
							<!-- Copyright -->
							<div class="copyright">
								<span><cms:text constant="copyright"/></span>
								<span><cms:text constant="rights"/></span>
							</div>

					</div>
			</footer>

		</body>
	</html>
</document>
